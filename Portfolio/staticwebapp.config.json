{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_app/*", "/static/*", "/favicon.ico", "/*.png", "/*.jpg", "/*.jpeg", "/*.svg", "/*.css", "/*.js"]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".map": "application/json",
    ".txt": "text/plain",
    ".ico": "image/x-icon",
    ".svg": "image/svg+xml",
    ".ttf": "font/ttf",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; font-src 'self' data:; img-src 'self' data:;",
    "X-Content-Type-Options": "nosniff"
  },
  "routes": [
    {
      "route": "/_app/*",
      "rewrite": "/_app/*"
    },
    {
      "route": "/static/*",
      "rewrite": "/static/*"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
